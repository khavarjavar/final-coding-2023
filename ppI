#include <stdio.h>
#include <math.h>
int k, n, res;
int f(int k, int n)
{
	if (k == n) return 1;
	if (k == 0) return 1;
	return f(k - 1, n - 1) + f(k, n - 1);
}
int main(void)
{
	scanf("%d %d", &k, &n);
	res = f(k, n);
	printf("%d\n", res);
	return 0;
}





